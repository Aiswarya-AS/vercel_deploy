{
    "builds": [{
        "src": "vercel_example/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_example/wsgi.py"
        }
    ],
    "env": {
        "VERCEL_RUNTIME_PYTHON_VERSION": "3.9"
    }
}